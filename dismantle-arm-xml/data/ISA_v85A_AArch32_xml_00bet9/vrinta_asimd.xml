      <ps_section howmany="1">
        <ps name="aarch32/instrs/VRINTA_asimd/A1_A.txt" mylink="aarch32.instrs.VRINTA_asimd.A1_A.txt" enclabels="" sections="1" secttype="noheading">
          <pstext mayhavelinks="1" section="Decode" rep_section="decode">if op&lt;2&gt; != op&lt;0&gt; then SEE "Related encodings";
if Q == '1' &amp;&amp; (Vd&lt;0&gt; == '1' || Vm&lt;0&gt; == '1') then UNDEFINED;
if (size == '01' &amp;&amp; !<a link="impl-shared.HaveFP16Ext.0" file="shared_pseudocode.xml" hover="function: boolean HaveFP16Ext()">HaveFP16Ext</a>()) || size IN {'00', '11'} then UNDEFINED;
// Rounding encoded differently from other VCVT and VRINT instructions
rounding = <a link="impl-shared.FPDecodeRM.1" file="shared_pseudocode.xml" hover="function: FPRounding FPDecodeRM(bits(2) rm)">FPDecodeRM</a>(op&lt;2&gt;:NOT(op&lt;1&gt;));  exact = FALSE;
case size of
    when '01' esize = 16; elements = 4;
    when '10' esize = 32; elements = 2;
d = <a link="impl-shared.UInt.1" file="shared_pseudocode.xml" hover="function: integer UInt(bits(N) x)">UInt</a>(D:Vd);  m = <a link="impl-shared.UInt.1" file="shared_pseudocode.xml" hover="function: integer UInt(bits(N) x)">UInt</a>(M:Vm);  regs = if Q == '0' then 1 else 2;</pstext>
        </ps>
